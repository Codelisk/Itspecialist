<UserControl x:Class="ModuleShell.Views.Uno.Views.Headers.ShellHeader"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="using:ModuleShell.Views.Uno.Views.Headers"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="300"
             d:DesignWidth="400"
             mc:Ignorable="d">

    <StackPanel Orientation="Horizontal">
        <Button Command="{Binding SetupCommand}"
                Content="Setup" />
        <Button Command="{Binding OpportunityCommand}"
                Content="Jobs" />
        <Button Command="{Binding TalentCommand}"
                Content="Talente" />
    </StackPanel>
</UserControl>
